
public class Legos360 {
public static void main(String[] args) {
	String[] jogosLego360 = new String [] {"Minecraft", "Lego Batman 3", "Lego Batman 2",
			"Lego Indiana Jones", "Lego Piratas Caribe", "Lego Avengers 3","Lego Movie"};
	
	
	System.out.println("Escolha o Jogo:");
	
	for (int i =0; i < jogosLego360.length;i++) {
		System.out.println("["+i+"]"+jogosLego360[i]);
	}
	
	}
	
}

